endif
ifdef VBOX_WITH_AUTOMATIC_DEFS_QUOTING
 TEMPLATE_VBoxMesa3DGuestR3Lib_DEFS         = \
 	$(TEMPLATE_VBoxMesa3DGuestR3Dll_DEFS) \
