LIBRARY VBoxSF
DESCRIPTION 'VirtualBox Guest Additions Shared Folder FSD for OS/2.'
DATA PRELOAD SINGLE SHARED MOVABLE
CODE PRELOAD MOVABLE
EXPORTS
    ; Data exports
    FS_ATTRIBUTE
    FS_NAME
    FS_MPSAFEFLAGS2
    FS32_ATTRIBUTE

    ; 16-bit entry points.
    FS_ALLOCATEPAGESPACE
    FS_ATTACH
    FS_CANCELLOCKREQUEST
    FS_CANCELLOCKREQUESTL
    FS_CHDIR
    FS_CHGFILEPTR
    FS_CLOSE
    FS_COMMIT
    FS_COPY
    FS_DELETE
    FS_DOPAGEIO
    FS_EXIT
    FS_FILEATTRIBUTE
    FS_FILEINFO
    FS_FILEIO
    FS_FILELOCKS
    FS_FILELOCKSL
    FS_FINDCLOSE
    FS_FINDFIRST
    FS_FINDFROMNAME
    FS_FINDNEXT
    FS_FINDNOTIFYCLOSE
    FS_FINDNOTIFYFIRST
    FS_FINDNOTIFYNEXT
    FS_FLUSHBUF
    FS_FSCTL
    FS_FSINFO
    FS_INIT
    FS_IOCTL
    FS_MKDIR
    FS_MOUNT
    FS_MOVE
    FS_NEWSIZE
    FS_NEWSIZEL
    FS_NMPIPE
    FS_OPENCREATE
    FS_OPENPAGEFILE
    FS_PATHINFO
    FS_PROCESSNAME
    FS_READ
    FS_RMDIR
    FS_SETSWAP
    FS_SHUTDOWN
    FS_VERIFYUNCNAME
    FS_WRITE

    ; 32-bit entry points.
    FS32_CHGFILEPTRL
    FS32_READ
    FS32_READFILEATCACHE
    FS32_RETURNFILECACHE
    FS32_WRITE

