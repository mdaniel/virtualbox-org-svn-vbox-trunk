<?if $(env.VBOX_WITH_DRAG_AND_DROP)= "yes" ?>
        <File Id="file_VBoxDragAndDropSvc.dll" Name="VBoxDragAndDropSvc.dll"
              Source="$(env.PATH_OUT)\bin\VBoxDragAndDropSvc.dll" />
<?endif ?>
<?if $(env.VBOX_WITH_GUEST_PROPS) = "yes" ?>
