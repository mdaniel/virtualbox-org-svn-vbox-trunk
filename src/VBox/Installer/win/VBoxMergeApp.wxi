              Source="$(env.PATH_OUT)\bin\VBoxSupLib.dll" />
        <File Id="file_VBoxVMM.dll" Name="VBoxVMM.dll"
              Source="$(env.PATH_OUT)\bin\VBoxVMM.dll" />
<?if $(env.VBOX_WITH_VRDP) = "yes" ?>
