            g_afTypeSupports[T_SHA]      = RT_BOOL(fEbx & X86_CPUID_STEXT_FEATURE_EBX_SHA);
        }

        if (g_uBs3CpuDetected & BS3CPU_F_CPUID_EXT_LEAVES)
        {
