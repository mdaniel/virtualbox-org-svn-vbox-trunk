typedef X86YMMFLOATPSREG BS3_FAR *PX86YMMFLOATPSREG;
/** Pointer to a const YMM single-precision packed floating-point register. */
typedef X86YMMFLOATPSREG const BS3_FAR *PCX86YMMFLOATPSREG;

