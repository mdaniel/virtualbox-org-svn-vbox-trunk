    X86YMMFLOATPSREG    uSrc1;                  /**< uDstIn for SSE */
    X86YMMFLOATPSREG    uDstOut;                /**< Destination output. */
    uint32_t            fMxCsrMask;             /**< MXCSR exception mask. */
    uint32_t            fDenormalsAreZero;      /**< DAZ (Denormals-Are-Zero) exception mask. */
    uint32_t            fFlushToZero;           /**< Flush-To-Zero (FZ) exception mask. */
