;                 | sed -e 's/^ *%define *//' -e 's/^\([^() ]*\).*$/\1/' \
;                 | sort -u`
;       do
;               if ! grep -wF "%undef $i" bootsector2-template-footer.mac; then
;                       echo $i
;               fi
;       done
;
