include $(KBUILD_PATH)/subheader.kmk

LIBRARIES += providers_macs
providers_macs_TEMPLATE = SubLibCrypto
providers_macs_DEFS = OPENSSL_NO_DEPRECATED
providers_macs_SOURCES = \
	blake2b_mac.c \
	blake2s_mac.c \
	cmac_prov.c \
	gmac_prov.c \
	hmac_prov.c \
	kmac_prov.c \
	poly1305_prov.c \
	siphash_prov.c
$(evalcall VBOX_OPENSSL_X86,providers_macs)

